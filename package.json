{
  "name": "wrap-kerbside-lookup",
  "repository": "https://github.com/etchteam/wrap-kerbside-lookup",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "source": "src/components/pages/app.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --no-prerender --clean --template src/index.ejs --service-worker false",
    "serve": "npm run build && preact serve",
    "dev": "preact watch --template src/index.ejs",
    "dist": "microbundle --entry src/components/pages/app.js"
  },
  "keywords": [],
  "author": "Zouhir <zouhir@zouhir.org>",
  "license": "MIT",
  "devDependencies": {
    "dotenv-webpack": "1.7.0",
    "eslint": "^6.2.2",
    "eslint-config-standard": "14.1.0",
    "eslint-config-standard-preact": "1.1.6",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-standard": "4.0.1",
    "identity-obj-proxy": "^3.0.0",
    "if-env": "^1.0.0",
    "jest": "^21.2.1",
    "microbundle": "^0.11.0",
    "node-sass": "^4.12.0",
    "preact-cli": "^2.0.2",
    "preact-widget-scripts": "^0.6.0",
    "sass-loader": "^6.0.6"
  },
  "dependencies": {
    "lodash": "4.17.15",
    "preact": "^8.5.2",
    "preact-compat": "3.19.0",
    "preact-habitat": "^3.1.0",
    "preact-i18n": "1.4.1",
    "preact-render-to-string": "^4.1.0",
    "preact-router": "^2.5.7",
    "promise-polyfill": "8.1.3",
    "prop-types": "15.7.2",
    "reset-css": "5.0.1",
    "whatwg-fetch": "3.0.0"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|less)$": "identity-obj-proxy",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
