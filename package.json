{
  "name": "wrap-kerbside-lookup",
  "repository": "https://github.com/etchteam/wrap-kerbside-lookup",
  "version": "1.0.0",
  "description": "Kerbside recycling lookup micro widget",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "source": "src/index.js",
  "scripts": {
    "test": "jest",
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "NODE_OPTIONS=--openssl-legacy-provider preact build --prerender false --brotli true --template src/index.ejs --sw false",
    "serve": "npm run build && preact serve",
    "dev": "preact watch --template src/index.ejs",
    "dist": "microbundle --entry src/index.js --external none --define process.env.NODE_ENV=production,process.env.API_HOST=https://rl.recyclenow.com",
    "release": "semantic-release"
  },
  "keywords": [],
  "author": "Etch <hey@etch.co>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/plugin-proposal-do-expressions": "^7.22.5",
    "@babel/plugin-proposal-export-default-from": "^7.22.17",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.20.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.22.15",
    "@babel/plugin-transform-modules-commonjs": "^7.22.15",
    "@babel/plugin-transform-object-assign": "^7.22.5",
    "@babel/plugin-transform-react-constant-elements": "^7.22.5",
    "@babel/preset-env": "^7.22.20",
    "@etchteam/eslint-config": "^1.5.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-fetch": "^4.0.0",
    "dotenv-webpack": "^8.0.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-preact-pure": "^4.1.0",
    "eslint": "^8.49.0",
    "identity-obj-proxy": "^3.0.0",
    "if-env": "^1.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "microbundle": "^0.15.1",
    "preact-cli": "^3.5.0",
    "preact-widget-scripts": "^0.6.0",
    "sass": "^1.67.0",
    "sass-loader": "^10.2.0",
    "semantic-release": "^21.1.1"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "preact": "^10.17.1",
    "preact-habitat": "^3.1.0",
    "preact-i18n": "^2.4.0-preactx",
    "preact-render-to-string": "^6.2.1",
    "preact-router": "^4.1.2",
    "prop-types": "^15.8.1",
    "reset-css": "^5.0.2",
    "uk-postcode-validator": "^1.1.1"
  }
}
